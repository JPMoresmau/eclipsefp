<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="configuratorPages" name="%extPtConfiguratorPages_name" schema="schema/configuratorPages.exsd"/>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="%configuratorSet_label"
            description="%configuratorSet_desc"
            visible="true"
            id="net.sf.eclipsefp.common.ui.configurator.actionSet">
         <action
               disabledIcon="icons/full/dtool16/configurator.gif"
               toolbarPath="eclipsefpConfigurator"
               label="%configuratorAction_label"
               class="net.sf.eclipsefp.common.internal.ui.actions.RunConfiguratorDelegate"
               tooltip="%configuratorAction_tooltip"
               icon="icons/full/etool16/configurator.gif"
               style="push"
               id="net.sf.eclipsefp.common.internal.ui.actions.RunConfiguratorDelegate"/>
      </actionSet>
   </extension>   
   
   <!-- some common categories for eclipsefp subprojects to dock on -->
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%newWizardCategory_name"
            id="net.sf.eclipsefp.common.ui.newWizardCategory">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            name="%exportWizardCategory_name"
            id="net.sf.eclipsefp.common.ui.exportWizardCategory">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="%viewCategory_name"
            id="net.sf.eclipsefp.common.ui.fpViews">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            name="Functional Programming"
            id="net.sf.eclipsefp.common.ui.fpCheatSheets"/>
   </extension>
</plugin>
